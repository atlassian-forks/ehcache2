{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>atlassian-labs/server-platform-renovate-config:backend"
  ],
  "baseBranches": [
    "master"
  ],
  "packageRules": [
    {
      "description": "Disable updating of parent pom version",
      "matchPackageNames": [
        "net.sf.ehcache:ehcache-root",
        "net.sf.ehcache.internal:ehcache-terracotta-root"
      ],
      "enabled": false
    },
    {
      "baseBranchList": [
      ],
      "updateTypes": [
        "major",
        "minor"
      ],
      "enabled": false,
      "prConcurrentLimit": 5
    }
  ]
}